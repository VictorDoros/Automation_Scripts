{
  "name": "cypress-ts-template",
  "version": "1.0.0",
  "description": "Cypress + TypeScript testing framework template with POM and reports",
  "main": "index.js",
  "scripts": {
    "headlessMode": "cypress run",
    "lint": "npx eslint cypress/**/*.{ts,js}",
